{
  "name": "cuccioli-web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "BASE_URL=http://localhost:3000 nuxt-ts",
    "build": "BASE_URL=https://davidegiovanni.com nuxt-ts build",
    "start": "BASE_URL=https://davidegiovanni.com nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "@tailwindcss/typography": "^0.4.0",
    "core-js": "^3.14.0",
    "gsap": "^3.9.1",
    "husky": "^4.3.6",
    "js-yaml-loader": "^1.2.2",
    "nuxt": "^2.14.6",
    "nuxt-i18n": "^6.16.0",
    "query-string": "^7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.14.6",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/tailwindcss": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.10.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "nuxt-font-loader": "^1.1.3",
    "nuxt-gsap-module": "^1.5.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
